{
  "dashboard": {
    "id": null,
    "title": "Orchestrator Dashboard",
    "tags": ["orchestrator", "redis", "streams"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Redis Stream Lag",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_stream_lag_seconds",
            "legendFormat": "{{stream}} - {{consumer_group}} - {{consumer}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 10 },
                { "color": "red", "value": 30 }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Queue Depth by Stream",
        "type": "stat",
        "targets": [
          {
            "expr": "queue_depth",
            "legendFormat": "{{stream}} - {{consumer_group}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "red", "value": 500 }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Reprocessing Count",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(redis_reprocessing_total[5m])",
            "legendFormat": "{{stream}} - {{consumer_group}} - {{event_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Consumer Group Health",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_stream_lag_seconds > 30",
            "legendFormat": "{{stream}} - {{consumer_group}} - {{consumer}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null },
                { "color": "red", "value": 1 }
              ]
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Stream Processing Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(redis_stream_lag_seconds[5m])",
            "legendFormat": "{{stream}} - {{consumer_group}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}
