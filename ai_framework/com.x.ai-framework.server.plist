<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.x.ai-framework.server</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/env</string>
      <string>bash</string>
      <string>-lc</string>
      <string>cd "/Users/x/Desktop/AI COMPLETE AUTOMATION /V1 of builder/ai_framework" && source "../.venv/bin/activate" && python server.py</string>
