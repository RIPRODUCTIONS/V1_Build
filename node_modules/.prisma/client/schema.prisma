generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Run {
  id         String    @id @default(cuid())
  title      String
  status     String    @default("queued")
  startedAt  DateTime  @default(now())
  finishedAt DateTime?
}
